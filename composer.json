{
    "name": "unicodeveloper/laravel-quotes",
    "description": "A Laravel 5 Package for providing all kinds of quotes, from business to success to life to inspiring to DJKHALED Quotes",
    "keywords": ["php","github", "laravel","Open Source","quotes", "djkhaled", "success", "business", "inspiring","laravel-quotes"],
    "license": "MIT",
    "authors": [
      {
        "name": "unicodeveloper",
        "email": "prosperotemuyiwa@gmail.com"
      }
   ],
   "minimum-stability": "stable",
   "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.2.*",
    "guzzlehttp/guzzle": "~5.0"
   },
   "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
   "autoload": {
        "psr-4": {
          "Unicodeveloper\\Quotes\\": "src/"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "Unicodeveloper\\Quotes\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
